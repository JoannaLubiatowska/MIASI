      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape bpmnElement="showDegree" id="BPMNShape_showDegree">
        <omgdc:Bounds height="55.0" width="111.0" x="810.0" y="211.0"></omgdc:Bounds>
      </bpmndi:BPMNShape>
      <bpmndi:BPMNEdge bpmnElement="badScore" id="BPMNEdge_badScore">
        <omgdi:waypoint x="450.0" y="258.0"></omgdi:waypoint>
        <omgdi:waypoint x="450.0" y="293.0"></omgdi:waypoint>
        <omgdi:waypoint x="560.0" y="293.0"></omgdi:waypoint>
        <bpmndi:BPMNLabel>
          <omgdc:Bounds height="14.0" width="100.0" x="413.0" y="270.0"></omgdc:Bounds>
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge bpmnElement="flow8" id="BPMNEdge_flow8">
        <omgdi:waypoint x="665.0" y="188.0"></omgdi:waypoint>
        <omgdi:waypoint x="760.0" y="188.0"></omgdi:waypoint>
        <omgdi:waypoint x="760.0" y="218.0"></omgdi:waypoint>
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge bpmnElement="flow9" id="BPMNEdge_flow9">
        <omgdi:waypoint x="125.0" y="238.0"></omgdi:waypoint>
        <omgdi:waypoint x="230.0" y="238.0"></omgdi:waypoint>
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge bpmnElement="flow10" id="BPMNEdge_flow10">
        <omgdi:waypoint x="335.0" y="238.0"></omgdi:waypoint>
        <omgdi:waypoint x="430.0" y="238.0"></omgdi:waypoint>
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge bpmnElement="greatScore" id="BPMNEdge_greatScore">
        <omgdi:waypoint x="450.0" y="218.0"></omgdi:waypoint>
